echo '### --sqlandworker mysql'
### --sqlandworker mysql
  (sleep 2; parallel --sqlworker $MYSQLTBL sleep .3\;echo >$T1) &
  parallel --sqlandworker $MYSQLTBL sleep .3\;echo ::: {1..5} ::: {a..e} >$T2;   true sort -u $T1 $T2;   sql $MYSQL 'select * from parsql order by seq;'
