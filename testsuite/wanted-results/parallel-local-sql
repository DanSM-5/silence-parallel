echo '### --sqlandworker postgresql --results'
### --sqlandworker postgresql --results
  mkdir -p /tmp/out--sql
  (sleep 2; parallel --results /tmp/out--sql --sqlworker $PGTBL3 sleep .3\;echo >$T9) &
